{
  "name": "summonersconnect",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "GPL-3.0-only",
  "eslintConfig": {
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "no-dupe-args": 2,
      "no-empty": 2,
      "no-extra-parens": [
        2,
        "functions"
      ],
      "no-empty-character-class": 2,
      "no-invalid-regexp": 2,
      "no-unreachable": 2,
      "valid-jsdoc": 2,
      "curly": 2,
      "no-inner-declarations": 2,
      "no-cond-assign": 2,
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "block-spacing": [
        2,
        "always"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "keyword-spacing": 2,
      "no-duplicate-case": 2,
      "no-debugger": 2,
      "no-constant-condition": 2,
      "valid-typeof": 2,
      "no-alert": 2,
      "yoda": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxBOF": 0,
          "maxEOF": 1
        }
      ],
      "semi-spacing": 2,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-infix-ops": 2,
      "key-spacing": [
        2,
        {
          "singleLine": {
            "beforeColon": false,
            "afterColon": true
          },
          "multiLine": {
            "beforeColon": true,
            "afterColon": true,
            "align": "colon"
          }
        }
      ],
      "no-multi-str": "error",
      "new-parens": "error",
      "no-new-object": "error"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "got": "^11.5.2",
    "mariadb": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-util": "^3.0.8"
  }
}
